# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/followup_ai

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_S3_BUCKET_NAME=your_s3_bucket_name

# AWS Cognito Configuration
AWS_COGNITO_USER_POOL_ID=your_cognito_user_pool_id
AWS_COGNITO_CLIENT_ID=your_cognito_client_id
AWS_COGNITO_CLIENT_SECRET=your_cognito_client_secret
AWS_COGNITO_REGION=us-east-1
AWS_COGNITO_DOMAIN=your_cognito_domain

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_BAA_SIGNED=false
OPENAI_ZDR_ENABLED=false
OPENAI_ENTERPRISE=false

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Security
DEV_MODE_SECRET=your_dev_mode_secret_min_32_chars
SESSION_SECRET=your_session_secret

# Environment
NODE_ENV=development

# CORS Origins (comma-separated)
# CORS_ORIGINS=http://localhost:5000,http://127.0.0.1:5000
