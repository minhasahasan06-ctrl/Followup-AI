# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/followup_ai

# Google Cloud Platform Configuration
GCP_PROJECT_ID=your_gcp_project_id
GCP_REGION=us-central1
GCP_STORAGE_BUCKET=your_gcs_bucket_name
GCP_KMS_KEY_RING_ID=your_kms_key_ring
GCP_KMS_CRYPTO_KEY_ID=your_kms_key
# For local development, use service account JSON file:
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# OpenAI Configuration (HIPAA-compliant with BAA)
OPENAI_API_KEY=your_openai_api_key
OPENAI_BAA_SIGNED=false
OPENAI_ZDR_ENABLED=false
OPENAI_ENTERPRISE=false

# Stytch Authentication
STYTCH_PROJECT_ID=your_stytch_project_id
STYTCH_SECRET=your_stytch_secret
STYTCH_PUBLIC_TOKEN=your_stytch_public_token
STYTCH_ENV=test

# Stripe Payments
STRIPE_API_KEY=your_stripe_api_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Daily.co Video
DAILY_API_KEY=your_daily_api_key
DAILY_DOMAIN=your_domain.daily.co

# Security
DEV_MODE_SECRET=your_dev_mode_secret_min_32_chars
SESSION_SECRET=your_session_secret

# Environment
NODE_ENV=development

# Application URLs
APP_BASE_URL=http://localhost:8000
FRONTEND_BASE_URL=http://localhost:5000

# CORS Origins (comma-separated)
# CORS_ALLOWED_ORIGINS=http://localhost:5000,http://127.0.0.1:5000
